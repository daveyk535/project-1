<h1>This page shows all participating restaurants </h1>

<table class="table .table-striped">
<% @restaurants.each do |restaurant| %>
  <tr>
    <td>
      <p class="lead"><%= link_to(restaurant.name) %></p>
    </td>
</tr>
<% end %>
</table>

<%= link_to "Add Restaurant", new_restaurant_path, class: "btn btn-large btn-primary" %>











